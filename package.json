  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm-run-all lint test"
    }
  },
  "lint-staged": {
    "*.{js,ts,json}": [
      "prettier --write 'src/**/*.ts'",
      "git add"
    ]
  }
