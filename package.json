    "@types/morgan": "^1.7.36",
    "@types/node": "^12.7.2",
    "@types/nodemailer": "^6.2.1",
    "@types/sequelize": "^4.28.4",
    "@types/supertest": "^2.0.8",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
